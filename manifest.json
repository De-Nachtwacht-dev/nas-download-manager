{
  "manifest_version": 2,
  "name": "synology",
  "version": "0.1.0",
  "description": "",
  // "content_scripts": [{
  //   "matches": [ "<all_urls>" ],
  //   "js": [ "dist/bundle.js" ]
  // }],
  "background": {
    "scripts": [ "dist/background/background.js" ]
  },
  "permissions": [
    "storage",
    "notifications",
    "*://*.synology.me/*"
  ],
  "browser_action": {
    "browser_style": true,
    "default_title": "Download Station",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "64": "icons/icon-64.png",
      "256": "icons/icon-256.png"
    }
  },
  "options_ui": {
    "page": "html/settings.html"
  }
}

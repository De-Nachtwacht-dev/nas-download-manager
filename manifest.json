{
  "manifest_version": 2,
  "name": "synology",
  "version": "0.1.0",
  "description": "",
  "background": {
    "scripts": [ "dist/js/background/background.js" ]
  },
  "permissions": [
    "contextMenus",
    "runtime",
    "storage",
    "notifications",
    "*://*.synology.me/*",
    "*://*.DiskStation.me/*",
    "*://*.i234.me/*",
    "*://*.myDS.me/*",
    "*://*.DSCloud.biz/*",
    "*://*.DSCloud.me/*",
    "*://*.DSCloud.mobi/*",
    "*://*.DSmyNAS.com/*",
    "*://*.DSmyNAS.net/*",
    "*://*.DSmyNAS.org/*",
    "*://*.FamilyDS.com/*",
    "*://*.FamilyDS.net/*",
    "*://*.FamilyDS.org/*"
  ],
  "browser_action": {
    "browser_style": true,
    "default_title": "Download Station",
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "64": "icons/icon-64.png",
      "256": "icons/icon-256.png"
    }
  },
  "options_ui": {
    "page": "html/settings.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png",
    "256": "icons/icon-256.png"
  }
}

{
  "name": "synology-download-manager",
  "version": "0.1.0",
  "description": "An open source browser extension for adding/managing download tasks to your Synology DiskStation.",
  "scripts": {
    "test": "exit 0",
    "watch:styles": "scss --watch scss:dist/css",
    "build:styles": "scss --update scss:dist/css",
    "build:transpile": "tsc",
    "watch:transpile": "tsc -w",
    "build:bundle": "node build.js",
    "watch:bundle": "node build.js -w",
    "build": "npm-run-all build:transpile build:bundle build:styles",
    "watch": "npm-run-all watch:transpile watch:bundle watch:styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seansfkelley/synology-download-manager.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/lodash-es": "^4.14.5",
    "@types/query-string": "^4.3.1",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "scss": "^0.2.4",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "lodash-es": "^4.17.4",
    "moment": "^2.18.1",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "synology-typescript-api": "^0.1.1"
  }
}
